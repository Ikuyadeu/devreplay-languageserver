{
  "name": "devreplay-server",
  "version": "0.2.0",
  "author": "Yuki Ueda",
  "description": "Language server for DevReplay",
  "keywords": [
    "language server",
    "devreplay"
  ],
  "engines": {
    "node": "*"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devreplay/devreplay-languageserver.git"
  },
  "bin": {
    "devreplay-server": "./bin/devreplay-server"
  },
  "bugs": {
    "url": "https://github.com/devreplay/devreplay-languageserver/issues"
  },
  "dependencies": {
    "devreplay": "^1.2.9",
    "vscode-languageserver-textdocument": "^1.0.1",
    "vscode-languageserver": "6.1.1",
    "vscode-uri": "^2.1.1"
  },
  "devDependencies": {
    "typescript": "^3.8.3"
  },
  "scripts": {
    "prepublishOnly": "npm run clean && npm run compile",
    "compile": "npx gulp compile-extension:devreplay-language-features-server",
    "watch": "npx gulp watch-extension:devreplay-language-features-server",
    "clean": "../../../node_modules/.bin/rimraf out",
    "install-server-next": "yarn add vscode-languageserver@next",
    "install-server-local": "yarn link vscode-languageserver-server",
    "version": "git commit -m \"DevReplay Language Server $npm_package_version\" package.json"
  }
}
